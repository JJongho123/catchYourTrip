<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hyeho.trip.mapper.UserMapper">
    <select id="getUserList" resultType="com.hyeho.trip.vo.User">
        SELECT id
        , login_id
        FROM user
        ORDER by id asc
    </select>
</mapper>